{
  "name": "pagerank",
  "description": "Tool for finding the Google PageRank of a given url. Can be used as a node module or a cli command when installed with -g",
  "version": "2.0.0",
  "author": "Nathan Friedly - http://nfriedly.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/nfriedly/node-pagerank"
  },
  "main": "./lib/pagerank.js",
  "scripts": {
    "test": "jshint {lib,bin,spec}/*.js && tape spec/*.js | tap-spec"
  },
  "bin": {
    "pagerank": "./bin/cli.js"
  },
  "keywords": [
    "Google",
    "Pagerank",
    "SEO"
  ],
  "license": "MIT",
  "devDependencies": {
    "jshint": "^2.8.0",
